nav.navbar.navbar-default.navbar-fixed-top.navbar-custom
    .container-fluid
        .navbar-header
            button(class="navbar-toggle", type="button", data-toggle="collapse", data-target="#main-navbar")
                span.sr-only Toggle navigation
                span.icon-bar
                span.icon-bar
                span.icon-bar
            a(class="navbar-brand", href=url_for())
                span= config.title
        #main-navbar.collapse.navbar-collapse
            ul.nav.navbar-nav.navbar-right
                each value, key in theme["navbar-links"]
                    li
                        a(href=url_for(value))!= key
                li
                    a(href=theme.footer.twitter.link)
                        i.fa.fa-twitter.larger
                        span.visible-xs-inline.ml-1 Twitter
                li
                    a(href=theme.footer.github.link)
                        i.fa.fa-github.larger
                        span.visible-xs-inline.ml-1 Github
                li
                    a(href=theme.footer.youtube.link)
                        i.fa.fa-youtube.larger
                        span.visible-xs-inline.ml-1 YouTube